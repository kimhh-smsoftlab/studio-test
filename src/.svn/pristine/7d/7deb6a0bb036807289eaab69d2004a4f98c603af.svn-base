// [tab:event ] OnTabChanged  start...
tab_profit.OnTabChanged=function(nCurrentTab){
    $('.tabtitle.clicked').addClass('clicked');
    switch(nCurrentTab){
        case "0":
            formlink_screen.SetPage('bbb200051');
            break;
        case "1":
            formlink_screen.SetPage('bbb200052');
            break;
        case "2":
            formlink_screen.SetPage('bbb200053');
            break;
        case "3":
            formlink_screen.SetPage('bbb200054');
            break;
    }
};

// [form:event ] OnFormInit  start...
form.OnFormInit=function(tagName, objData){
    $('.tabtitle').css('border','none');
    $('.tabtitle').css('border-bottom','1px solid #000000');
};