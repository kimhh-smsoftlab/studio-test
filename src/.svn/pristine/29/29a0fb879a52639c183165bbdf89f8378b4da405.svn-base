.header-chart-panel .button.fullscreen{position:absolute;right:10px;background-color:inherit;border:0;}
.header-chart-panel .group.header-group-screenshot{display:none;}

.header-chart-panel .button.fullscreen svg{fill:#8a8b8d;}
.header-chart-panel .button.fullscreen svg:last-child{ display:none;margin-top:2px;}
.header-chart-panel .button.fullscreen.on svg:first-child{ display:none;}
.header-chart-panel .button.fullscreen.on svg:last-child{ display:inline-block;}

._tv-dialog,._tv-dialog-content{max-height:400px;}
._tv-dialog-content .alert-dialog-content, ._tv-dialog-content .main-properties{max-height:240px;padding:10px 25px;}

.tv-close-panel{display:none;}

.layout__area--left > div, .dark .layout__area--left > div{background-color:#1c283e;}
.light .layout__area--left > div{background-color:#25385f;}

.apply-common-tooltip, .dark .apply-common-tooltip{background-color: #1c283e!important;border-color: #1c283e!important;}
.light .apply-common-tooltip{background-color: #25385f!important;border-color: #25385f!important;}

.tv-close-panel.top{display:none;}
._tv-dialog,._tv-dialog-content{max-height:400px;}
._tv-dialog-content .alert-dialog-content, ._tv-dialog-content .main-properties{max-height:240px;padding:10px 25px;}

.chart-container, .dark .chart-container{border-color:#1c283e!important;background-color:#1c283e!important;}
.light .chart-container{border-color:#25385f!important;background-color:#25385f!important;}

.chart-controls-bar, .dark .chart-controls-bar{background-color:#1c283e;}
.light .chart-controls-bar{background-color:#25385f;}

.chart-page, .dark.chart-page{background-color:#1c283e;}
.light.chart-page{background-color:#25385f;}

.charts-popup-list.intervals-list.favored-list{
	left: 330px!important;
	top: 35px!important;
}