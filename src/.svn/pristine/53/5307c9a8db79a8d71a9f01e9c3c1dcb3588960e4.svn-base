*{box-sizing:border-box;padding: 0;margin: 0;outline:none;}

body {
    position:absolute;
    width:100%;
    height:100%;
    -webkit-user-select: none;
    -moz-user-select: -moz-none;
    -ms-user-select: none;
    -khtml-user-select: none;
    -o-user-select: none;
}

input,
textarea {
     -moz-user-select: text;
}

input::-ms-clear {
    display: none;
    width: 0;
    height: 0;
}

#hi5_loading{ position:fixed; width:100%; height:100%; z-index:99999999; background:url(../images/loading.gif) no-repeat center; }
#hi5_content{
  height:100%;
  width:100%;
  overflow:auto;
  font-size:12px;
}

.up_txt{color:#f11421 !important}      /*상승*/
.low_txt{color:#9ac8fc !important}     /*하락*/
.balance_txt{color:#000000 !important} /*보합*/

/* control css */
/*account*/
.hi5_account{
    position:absolute;
    display: block;
    overflow:hidden;
    font:inherit;
    padding: 0px;
    border-radius: 0;
    background-color:#FFFFFF;
    color:#202020;
    border:1px solid #ACACAC;
}

.hi5_account:disabled{
    background-color:#D7D8DA;
    color:#929292;
    border-color:#ACACAC;
    cursor:not-allowed;
}
/*********/
/*button*/
.hi5_button {
    position:absolute;
    overflow:hidden;
    border-width:1px;
    border-style:solid;
    border-radius:2px;
    cursor:pointer;
    font:inherit;
    white-space:nowrap;
    background-repeat:no-repeat;
    background-size:auto;
    background-position:left top;
    border-color:#ACACAC;
    background-color:#DBDBDB;
    color:#202020;
}
.hi5_button:hover{
    border-color:#81868B;
    background-color:#D2DDE8;
    color:#202020;
}
.hi5_button:disabled{
    border-color:#ACACAC;
    background-color:#DBDBDB;
    color:#929292;
    cursor:not-allowed;
}
.hi5_button.btn_buy{
    border-color:#DC3B3A;
    color:#FFFFFF;
    background:linear-gradient(#F35F5E,#ED4342);
}
.hi5_button.btn_buy:hover{
    border-color:#CF201F;
    color:#202020;
    background:linear-gradient(#EE3E3D,#E62625);
}
.hi5_button.btn_buy:disabled{
    border-color:#8B8B8B;
    color:#FFFFFF;
    background:linear-gradient(#A8A8A8,#979797);
}
.hi5_button.btn_sell{
    border-color:#3A5CDC;
    color:#FFFFFF;
    background:linear-gradient(#5E8BF3,#426AED);
}
.hi5_button.btn_sell:hover{
    border-color:#1F3BCF;
    color:#FFFFFF;
    background:linear-gradient(#3D6BEE,#2549E6);
}
.hi5_button.btn_sell:disabled{
    border-color:#8B8B8B;
    color:#FFFFFF;
    background:linear-gradient(#A8A8A8,#979797);
}
.hi5_button.btn_modify{
    border-color:#35A48A;
    color:#FFFFFF;
    background:linear-gradient(#4FC6AA,#2CA68A);
}
.hi5_button.btn_modify:hover{
    border-color:#1B886A;
    color:#FFFFFF;
    background:linear-gradient(#30B28F,#158A6A);
}
.hi5_button.btn_modify:disabled{
    border-color:#8B8B8B;
    color:#FFFFFF;
    background:linear-gradient(#A8A8A8,#979797);
}
.hi5_button.btn_cancel{
    border:0;
    color:#adb2ba;
    background:#343f54;
}
.hi5_button.btn_cancel:hover{
    border:0;
    color:#adb2ba;
    background:#343f54;
}
.hi5_button.btn_cancel:disabled{
    border-color:#8B8B8B;
    color:#FFFFFF;
    background:linear-gradient(#A8A8A8,#979797);
}
.hi5_button .toggleOn {
    border-style:inset;
    outline:none;
}
.hi5_button .toggleOff{
    border-style:outset;
    outline:none;
}
/********/
/*calendar*/
/*base*/
.hi5_calendar{
    position: absolute;
    color:#202020;
    /*background-color:#ffffff;*/
    border-color:#acacac;
}
.hi5_calendar[disabled]{
    background-color:#D7D8DA;
}
.hi5_calendar input:disabled{
    cursor:not-allowed;
}
.hi5_calendar input{
    width:100%; 
    height:100%;
    border-width:1px;
    border-style: solid;
    border-color: inherit;
    font:inherit;
    padding-left:5px;
    color: inherit;
    background-color: inherit;
}
/*image on right*/
.hi5_calendar.right_cal input{
    margin-right: -24.5px;
    float:left;
    padding:0;
    padding-left: 5px;
}
.hi5_calendar.right_cal .ui-datepicker-trigger{
    width:23px;
    height:93%;
    margin-top:1px;
    float:left;
    background-color:transparent;
    border:0;
    font-size: 16px;
}
/*image on left*/
.hi5_calendar.left_cal input{
    text-indent: 23px;
    float:right;
    padding:0;
}
.hi5_calendar.left_cal .ui-datepicker-trigger{
    width:23px;
    height:95%;
    margin-top:1px;
    float:right;
    margin-right:-23.5px;
    background-color:transparent;
    border:0;
    font-size: 16px;
}
/* 월별 선택 */
.hi5_calendar i{
    float:left;
    height:100%;
    width:20px;
    font-size:18px;
    cursor:pointer;
}
.hi5_calendar .calShowText{
    float:left;
    height:100%;
    width: calc(100% - 40px);
    text-align:center;
    font-size:24px;
}
/***************/
/*chart*/
.hi5_chart{
    position:absolute;
    overflow:hidden;
    border:1px solid #ACACAC;
    background-color:#ffffff;
    color:#202020;
}
/***************/
/*checkbox*/
.hi5_checkbox, .hi5_checkbox-left, .hi5_checkbox-right{
    position:absolute;
    border:1px solid #ACACAC;
    overflow:hidden;
    background-color:inherit;
    color:#202020;
    text-align:left;
}
.hi5_checkbox table, .hi5_checkbox-left table, .hi5_checkbox-right table{
    table-layout:fixed;
    white-space:nowrap;
    background-color:inherit;
    width:100%;
    height:100%;
}
.hi5_checkbox input {
    padding:5px;
    vertical-align: middle;
}
.hi5_checkbox input:disabled{
    cursor:not-allowed;
}
.hi5_checkbox-left[disabled="disabled"] table,
.hi5_checkbox-left[disabled="disabled"] td,
.hi5_checkbox-left[disabled="disabled"] label{
    cursor:not-allowed!important;
}
.hi5_checkbox td, .hi5_checkbox-left td, .hi5_checkbox-right td{
    overflow:hidden;
    white-space:nowrap;
    line-height:18px;
}
.hi5_checkbox-right td{
    text-align:right;
}
/* valueB 거래소 checkbox 디자인 수정 */

.adm-left input[type=checkbox] {
    opacity:0;
    float:left;
    width:20px;
}
.adm-left input[type=checkbox]:disabled {
    cursor:not-allowed;
}

.adm-left input[type=checkbox] + label{
    margin-left:-20px;
    clear:none;
    padding:5px 0 4px 24px;
    cursor:pointer;
}
.adm-left input[type=checkbox]:checked+label{
    cursor:pointer;
}


/* valueB 거래소 checkbox 디자인 수정 */

.adm-right input[type=checkbox] {
    opacity:0;
    float:right;
    width:20px;
}
.adm-right input[type=checkbox]:disabled {
    cursor:not-allowed;
}
.adm-right label{
    margin-right: -20px;
    clear: none;
    padding: 5px 24px 4px 0px;
    cursor: pointer;
}
.adm-right input[type=checkbox]:checked + label{
   cursor:pointer;
}

/*combo*/
.hi5_combo {
    position:absolute;
    overflow:hidden;
    border:1px solid #ACACAC;
    text-align:left;
    font:inherit;
    white-space:nowrap;
    background-color:#ffffff;
    color:#202020;
}
.hi5_combo:disabled{
    cursor:not-allowed;
    background-color:#D7D8DA;
    color:#ACACAC;
}
/***************/
/*div*/
.hi5_div{
    position:absolute;
    font:inherit;
    overflow:hidden;
    border:0px solid #ACACAC;
    font:inherit;
    background-color:inherit;
    color :inherit;
}
/***************/
/*multiline edit*/
.hi5_edit{
    border:1px solid #ACACAC;
    color:#202020;
    background-color:#FFFFFF;
    position:absolute;
    padding:6px;
    resize:none;
    font:inherit;
    overflow-y:hidden;
    word-wrap:break-word;
    ime-mode:active;
    -ms-ime-mode:active;
    -moz-ime-mode:active;
    -khtml-ime-mode: active;
    -webkit-ime-mode: active;
    -o-user-ime-mode: active;
}
.hi5_edit:disabled {
    border:1px solid #ACACAC;
    color:#929292;
    background-color:#DBDBDB;
}
/***********/
/*formlink*/
.hi5_formlink{
    position:absolute;
    overflow:hidden;
    border:none;
    font:inherit;
    border-color: #ACACAC;
    color: #202020;
}
/**********/
/*groupbox*/
.hi5_groupbox{
    position:absolute;
    display:block;
    -webkit-box-sizing:border-box;
    -moz-box-sizing:border-box;
    box-sizing:border-box;
}
.hi5_admin .hi5_groupbox{
    border:1px solid #ACACAC;
    background-color:#FFFFFF;
}
.hi5_groupbox.ordernow{
    overflow-y:auto;
}
/***************/
/*image*/
.hi5_image {
    border: 0px;
    cursor: default;

    -moz-user-select: none;
    -khtml-user-select: none;
    -webkit-user-select: none;
    -o-user-select: none;

    position:absolute;
    overflow:hidden;
    background-color:#FFFFFF;
    border:0px solid #FFFFFF;
    background-repeat:no-repeat;
    background-position:left top;
    background-size:100% 100%;
}
/****************/
/*jmcombo*/
.hi5_jmcombo_WTS, .hi5_jmcombo_SPA, .hi5_jmcombo_MTS{
    position:absolute;
    border:1px solid #ACACAC;
    background-color:#FFFFFF;
    color:#202020;
    font:inherit;
}
/***************/
/*label*/
.hi5_label{
    position:absolute;
    overflow:hidden;
    font:inherit;
    border:1px solid #ACACAC;
    color:#202020;
    background-color:inherit;
    display:block;
    cursor:default;
    padding-left:4px;
    padding-right:4px;

    -webkit-user-select: none; /* Chrome/Safari */
    -moz-user-select: none; /* Firefox */
    -ms-user-select: none; /* IE10+ */
    user-select:none;
    -o-user-select:none;
    outline-style:none;
}
.hi5_label:disabled{
    cursor:not-allowed;
    background-color:#D7D8DA;
    color:#929292;
}
.hi5_label div:not(.currency-logo-sprite){
    display:table;
    width:100%;
    height:100%;
}
.hi5_label div div{
    display:table-cell!important;
    vertical-align:middle;
    text-align:left;
}

/***************/
/*maskedit*/
.hi5_maskedit{
    position:absolute;
    background-color:#ffffff;
    color:#202020;
    border:1px solid #ACACAC;
    text-align:left;
    font:inherit;
    white-space:nowrap;
    padding-left:4px;
    padding-right:4px;
    padding-bottom:1px;
}
.hi5_maskedit:disabled {
    border:1px solid #ACACAC;
    color:#929292;
    background-color:#D7D8DA;
}
/*********/
/*password*/
.hi5_password{
    position:absolute;
    text-align:left;
    font:inherit;
    ime-mode:inactive;
    padding-left:5px;
    border:1px solid #ACACAC;
    background-color:#FFFFFF;
    color:#202020;
}
.hi5_password:disabled{
    border-color:#acacac;
    background-color:#DBDBDB;
    color:#929292;
}
/**********/
/*radio*/
.hi5_radio{
    position:absolute;
    overflow:hidden;
    border:1px solid #ACACAC;
    white-space:nowrap;
    background-color:inherit;
}
.hi5_radio table{
    width:100%;
    height:100%;
    background-color:inherit;
}
.hi5_radio label > input{
    margin-top:-1px;
    vertical-align:middle;
    height:100%;
    margin-right:2px;
}
.radio-image, .radio-button, .radio-checkbox td{
    line-height:18px;
}
.radio-image input, .radio-checkbox input{
    visibility:hidden;
    position:absolute;
}

/* valueB 거래소 checkbox 디자인 수정 */
/*기존 css==============================================
.radio-image i{
    display:inline-block;
    vertical-align:middle;
    width:16px;
    height:16px;
    margin-right:5px;
}
==================================================== */
.radio-image i:before{
    display: inline-block;
    vertical-align: top;
    margin-right: 5px;
    font-family: FontAwesome;
    font-size: 15px;
    content: "\f111";
    font-style: normal;
}
    
.radio-image input:checked + i:before{
    content: "\f192";
}
.radio-checkbox i{
    display:inline-block;
    vertical-align:middle;
    width:16px;
    height:16px;
    margin-right:5px;
}
.radio-checkbox input:checked + i{
    width:16px;
    height:16px;
}

.radio-button table{
    /*border-collapse:collapse;*/
    border-spacing:0px;
}
.radio-button input, .radio-button i{
    display:none;
}
.radio-button td{
    /*border-right:1px solid rgb(218,218,218);*/
    border-right:1px solid #2f394b;
    vertical-align:middle;
    cursor:pointer;
    border-radius: 0 0 0 0;
}
.radio-button td:last-child{
    border-right:none;
}
.radio-button label{
    display:inline-block;
	text-align: center;
	/*text-shadow: 0 1px 1px rgba(255,255,255,0.75);*/
	cursor: pointer;
    width:100%;
    margin-bottom: 0;
    font-weight: normal;
    font-size:1em;
    color:inherit;
}
.radio-button input:checked + label{
	outline: 0;
}
.hi5_radio.radio-button .selected{
	background-color:#2e67cc;
    /*background-color:#C5D2DC;*/
    color:#FFFFFF;
}

.hi5_radio.radio-button {
	background-color: #343f54;
    color:#FFFFFF;
}
/*******/
/*slider*/
.hi5_slider{
    position:absolute;
    padding-top: 20px;
}
/********/
/* spin */
.hi5_spin,.hi5_spin-mobile{
    position:absolute;
    overflow:hidden;
    border:1px solid #cbcbcb;
    padding-right: 7px;
    padding-left: 7px;
}

.hi5_spin .spinBoxText{
    position:relative;
    text-align: right;
    width:calc(100% - 45px);
    font:inherit;
    border:none;
    float:right;
    height:100%;
    padding-right:5px;
    margin-right: 3px;
}

.hi5_spin .spinBoxUp{
    position: relative;
    float:right;
    width:20px;
    cursor: pointer;
    height: 50%;
    margin-top: 4%;
}
.hi5_spin .spinBoxDown{
    position: relative;
    float:left;
    width:20px;
    cursor: pointer;
    height: 50%;
    margin-top: 4%;
}

.hi5_spin > *:disabled{
    cursor:not-allowed;
}
/********/
/*static*/
.hi5_static{
    position:absolute;
    overflow:hidden;
    display:block;
    border:0px solid #ACACAC;
    background-color:inherit;
    color:#202020;
}
.hi5_static div{
    display:table;
    width:100%;
    height:100%;
}
.hi5_static div > div{
    display:table-cell;
    vertical-align:middle;
    text-align:left;
    white-space:nowrap;
}
/********/
/*tab*/
.hi5_tab{
    position:absolute;
    overflow:hidden;
    /*background-color:#E5E5E5;*/
    color:#333333;
    border:1px solid #ACACAC;
}
.hi5_tab .tabheader{
    margin:0px;
    position:relative;
    top:0px;
    left:0px;
    outline:none;
}
.hi5_tab .tabheader .tabheaderContainer{
    position:relative;
    top:0px;
    left:0px;
    width:101%;
    height:100%;
    white-space:nowrap;
    outline:none;
    border:none;
    text-align:center;
}
.hi5_tab .tabheader .tabheaderContainer .tabtitle{
    height:100%;
    float : left;
    position: static;
    padding:0 10px;
    list-style:none;
    cursor:pointer;
    display:inline-block;
    white-space:nowrap;
    border-right:1px solid #5F5F5F;
    background-color:#F5F5F5;
}
.hi5_tab .tabheader .tabheaderContainer .tabtitle.clicked{
    background-color:#9B9B9B;
    color:#FFFFFF;
}
.hi5_tab .tabcontents{
    position:relative;
    top:0px;
    left:0px;
    width:100%;
    float:none;
    box-sizing:border-box;
    border:1px solid #ACACAC;
    background-color:inherit;
    overflow:auto;
}
.hi5_tab .tabcontents .tabscreen{
    height:100%;
    width:100%;
}
/**********/
/*tvchart*/
.hi5_tvchart{
    position:absolute;
    overflow:hidden;
    border:1px solid #acacac;
    font:inherit;
    color:#202020;
    background-color:inherit;
}
/*web*/
.hi5_web {
    position: absolute;
    border: 1px solid #ACACAC;
    background-color : #DBDBDB;
}
/**********/

/* 호가 테이블 관련 */
.hoga_tbl{border-collapse: collapse; border-spacing: 0px;}
.hoga_tbl th,th{font-weight:normal;overflow:hidden;}
.hoga_tbl td,th{
    font-weight:normal; padding:0 3px; text-align:right; /*overflow:hidden;*/white-space:nowrap;text-overflow: ellipsis;
    line-height:17px;vertical-align:middle;height:20px;
}

.curframe_up{ outline: 2px solid #f00 !important; outline-offset : -2px !important;font-weight :bold !important}
.curframe_down{ outline: 2px solid #00f !important; outline-offset: -2px !important;font-weight :bold !important}
.curframe_euq{ outline: 2px solid #000 !important; outline-offset: -2px !important;font-weight :bold !important;}

.hoga_prc_sell_cell{ background-color: #b2dfff !important;font-weight :bold !important;}
.hoga_prc_buy_cell{ background-color: #ffc3d6 !important;font-weight :bold !important;}

/*잔량그래프,텍스트 css*/
.RightQtyDiv{letter-spacing: 1px;text-align: right;position: relative;height: 90%;}
.LeftQtyDiv {letter-spacing: 1px;text-align: left;position: relative;height: 90%;}
.SellQtyGraph{display:inline-block;background-color:#d3e4f7;vertical-align: middle;text-indent: 999em;text-align: right;height: 100%;line-height: inherit;}
.BuyQtyGraph{display:inline-block;background-color:#f8d8d7;vertical-align: middle;text-indent: 999em;text-align: left;height: 100%;line-height: inherit;}

.RightQtyText{position:absolute;top: 10%;right:2px;display:inline-block;height: inherit;line-height: inherit;}
.LeftQtyText{position:absolute;top: 10%;left:2px;display:inline-block;height: inherit;line-height: inherit;}

.hoga_tbl td.sum{letter-spacing:-1px;padding:0 !important;text-align:center}
.hoga_tbl tr.lsm > td{letter-spacing:-1px}
.hoga_tbl tr.hoga_head > td{text-align:center !important}

.hoga_tbl .pt td{*padding-top:0;*height:23px;*vertical-align:bottom} /*ie7*/
.hoga-tbl .pb td{*padding-bottom:0; *height:23px; *vertical-align:top }/*ie7*/
.hoga_tbl tr.hoga_head td{height:20px;line-height:20px;text-align:center;padding:0}
/*.hoga_tbl td{height:18px;line-height:18px}*/

.hoga_tbl_theme th,
.hoga_tbl_theme tr.hoga_head td{background:#ededed;border-left:1px solid #ccc;border-bottom:1px solid #aeaeae;color:#000}
.hoga_tbl_theme td{border-left:1px solid #e0e1e3}
.hoga_tbl_theme th:first-child,
.hoga_tbl_theme td:first-child,
.hoga_tbl_theme tr.hoga_head td:first-child{border-left:none}
.hoga_tbl_theme td.bgc{background:#f3f3f3}

